<?xml version="1.0"?>
<containers:Divider xmlns:fx="http://ns.adobe.com/mxml/2009"
                    xmlns:s="library://ns.adobe.com/flex/spark"
                    xmlns:containers="spark.containers.*" percentHeight="100">
    <s:Rect bottom="0" top="0" percentHeight="100" width="6">
        <s:fill>
            <s:SolidColor color="#7096ab"/>
        </s:fill>
    </s:Rect>
    <s:VGroup horizontalAlign="center" verticalAlign="middle" percentHeight="100">
        <s:Group>
            <s:BitmapImage id="img" source="@Embed('../../assets/divider.png')"/>
            <s:Rect width="6" height="16">
                <s:fill>
                    <s:SolidColor alpha="0" color="0x000000"/>
                </s:fill>
            </s:Rect>
        </s:Group>
    </s:VGroup>
</containers:Divider>
